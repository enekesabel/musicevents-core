{
  "defaultSeverity": "warning",
  "extends": [
    "tslint-config-airbnb"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**"
    ]
  },
  "rules": {
    "no-increment-decrement": [
      false
    ],
    "object-curly-spacing": [
      false
    ],
    "ter-indent": [
      true,
      "tab",
      1
    ],
    "import-name": [
      false
    ],
    "align": [
      false,
      "arguments"
    ]
  }
}
